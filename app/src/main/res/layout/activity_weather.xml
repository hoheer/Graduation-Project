<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Weather">

<RelativeLayout
    android:id="@+id/r1_1"
    android:layout_width="match_parent"
    android:layout_height="70dp"
    android:orientation="vertical"
    tools:ignore="MissingConstraints" />

    <RelativeLayout
        app:layout_constraintTop_toBottomOf="@+id/r1_1"
        android:id="@+id/r1"
        android:layout_width="match_parent"
        android:layout_height="438dp"
        android:background="@drawable/walk_orange_line"
        android:orientation="vertical"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-10dp"
        tools:layout_editor_absoluteY="4dp">

        <TextView
            android:id="@+id/gps"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="8dp"
            android:text="현위치"
            android:textSize="30dp" />

        <TextView
            android:id="@+id/temp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/gps"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            android:text="기온"
            android:textSize="25dp" />

        <TextView
            android:id="@+id/hum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/gps"
            android:layout_marginLeft="130dp"
            android:layout_marginTop="40dp"
            android:layout_toRightOf="@+id/temp"
            android:text="습도"

            android:textSize="25dp" />

        <TextView
            android:id="@+id/temp1"
            android:layout_width="wrap_content"
            android:layout_height="155dp"
            android:layout_below="@+id/temp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="20dp"
            android:text="기이오온"
            android:textSize="40dp" />

        <TextView
            android:id="@+id/hum1"
            android:layout_width="wrap_content"
            android:layout_height="155dp"
            android:layout_below="@+id/hum"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="20dp"
            android:layout_toRightOf="@+id/temp1"
            android:text="스읍도오"
            android:textSize="40dp" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/r2"
        android:layout_width="match_parent"
        android:layout_height="220dp"

        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/r1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp">

        <TextView
            android:id="@+id/fine_dust"
            android:layout_width="153dp"
            android:layout_height="155dp"
            android:background="@drawable/walk_green_dust1"
            android:text="미세먼지"
            android:gravity="center_horizontal"

            android:layout_marginTop="30dp"
            android:layout_marginLeft="40dp"
            android:textSize="30dp" />
<TextView
    android:background="@drawable/walk_green_dust2"
    android:id="@+id/fime_dust_matter"
    android:layout_width="153dp"
    android:layout_height="155dp"
    android:text="초 미세먼지"
    android:gravity="center_horizontal"
    android:textSize="30dp"
    android:layout_marginTop="30dp"
    android:layout_marginLeft="30dp"
    android:layout_toRightOf="@+id/fine_dust"/>

    </RelativeLayout>






</androidx.constraintlayout.widget.ConstraintLayout>